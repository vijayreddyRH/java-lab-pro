package sem3JavaLab;
import java.util.Scanner;
public class PrimeExample
{
public static void main(String args[])
{
 int i,m=0,flag=0;
 System.out.println("Enter integer value");
 Scanner sc = new Scanner(System.in);
 int n=sc.nextInt();
 m=n/2;
 if(n==0)
 {
 System.out.println(n+" is not prime number");
 }else if(n==1)
 {
 System.out.println("The given integer is
divisible by 1 or itself");
 }
 else
 {
 for(i=2;i<=m;i++)
 {
 if(n%i==0){
 System.out.println(n+" is divisible by some
integer");
 flag=1;
 break;
 }
 }
 if(flag==0) { System.out.println(n+" is not
divisible by any integer");
 }
 }//end of else
}
}
